<Signup>:
    name: "signup"
    MDFloatLayout:
        md_bg_color: 1, 1, 1, 1

        MDIconButton:
            icon: "arrow-left"
            theme_font_size: "Custom"
            font_size: "23.5sp"
            size_hint: None, None
            size: dp(45), dp(45)
            theme_text_color: "Custom"
            text_color: (0.1, 0.2, 0.4, 1)
            pos_hint: {"center_x": 0.125, "center_y": 0.94}
            on_release: app.go_back()
        
        MDLabel:
            text: "Sign Up"
            pos_hint: {"center_x": 0.6, "center_y": 0.875}
            theme_text_color: "Custom"
            text_color: 0.1, 0.2, 0.4, 1
            theme_font_size: "Custom"
            font_size: "28sp"
            halign: "left"
            theme_font_name: "Custom"
            font_name: "Poppins-Bold"
        MDLabel:
            text: "Lengkapi data dirimu dulu ya!"
            pos_hint: {"center_x": 0.6, "center_y": 0.825}
            theme_text_color: "Custom"
            text_color: 0.1, 0.2, 0.4, 1
            theme_font_size: "Custom"
            font_size: "14sp"
            halign: "left"
            theme_font_name: "Custom"
            font_name: "Poppins-Medium"
        
        ScrollView:
            do_scroll_x: False
            do_scroll_y: True
            size_hint: 1, .615
            pos_hint: {"center_x": .5, "center_y": .5}
            bar_width: 2
            effect_cls: "ScrollEffect"
            MDBoxLayout:
                orientation: "vertical"
                size_hint_y: None
                height: self.minimum_height
                padding: dp(15)
                spacing: dp(18)
                MDBoxLayout:
                    orientation: "vertical"
                    spacing: dp(2)
                    size_hint_y: None
                    height: self.minimum_height
                    MDLabel:
                        text: "Nama Lengkap (Sesuai KTP)"
                        size_hint_y: None
                        height: self.texture_size[1]
                        theme_text_color: "Custom"
                        text_color: (0.1, 0.2, 0.4, 1)
                        theme_font_size: "Custom"
                        font_size: "11sp"
                        halign: "left"
                        theme_font_name: "Custom"
                        font_name: "Poppins-SemiBold"
                        size_hint_x: None
                        width: dp(300)
                        padding: [dp(24), 0]
                    MDCard:
                        id: nama_card
                        style: "outlined"
                        size_hint: .87, None
                        height: dp(54)
                        pos_hint: {"center_x": 0.5, "center_y": 0.7}
                        theme_line_color: "Custom"
                        line_color: (0.1, 0.2, 0.4, 1)
                        line_width: 1.5
                        theme_bg_color: "Custom"
                        radius: [dp(10),]
                        padding: [dp(12),]
                        md_bg_color: (1, 1, 1, 1)
                        focus_color: (0, 0, 0, 0)
                        clip: True
                        MDIcon:
                            id: nama_icon
                            icon: "card-account-details"
                            theme_icon_color: "Custom"
                            icon_color: (0.1, 0.2, 0.4, 1)
                            pos_hint: {"center_x": 0.2, "center_y": 0.5}
                            theme_font_size: "Custom"
                            font_size: "23sp"
                        MDTextField:
                            id: nama
                            mode: "outlined"
                            text: "Masukkan nama lengkap"
                            size_hint_x: 1
                            radius: [dp(10),]
                            pos_hint: {"center_x": .5, "center_y": .465}
                            theme_text_color: "Custom"
                            text_color_normal: (0.1, 0.2, 0.4, 0.85)
                            text_color_focus: (0.1, 0.2, 0.4, 1)
                            theme_font_name: "Custom"
                            theme_font_size: "Custom"
                            font_size: "13.5sp"
                            font_name: "Poppins-Regular"
                            theme_line_color: "Custom"
                            line_color_normal: (0, 0, 0, 0)
                            line_color_focus: (0, 0, 0, 0)
                            theme_bg_color: "Custom"
                            fill_color_normal: (1, 1, 1, 0)
                            fill_color_focus: (1, 1, 1, 0)
                            on_focus: root.update_text(self, self.focus, "Masukkan nama lengkap")
                            on_text: self.error = False
                
                MDBoxLayout:
                    orientation: "vertical"
                    spacing: dp(2)
                    size_hint_y: None
                    height: self.minimum_height
                    MDLabel:
                        text: "Tanggal Lahir (Sesuai KTP)"
                        size_hint_y: None
                        height: self.texture_size[1]
                        theme_text_color: "Custom"
                        text_color: (0.1, 0.2, 0.4, 1)
                        theme_font_size: "Custom"
                        font_size: "11sp"
                        halign: "left"
                        theme_font_name: "Custom"
                        font_name: "Poppins-SemiBold"
                        size_hint_x: None
                        width: dp(300)
                        padding: [dp(24), 0]
                    MDCard:
                        id: tgl_card
                        style: "outlined"
                        size_hint: .87, None
                        height: dp(54)
                        pos_hint: {"center_x": 0.5, "center_y": 0.7}
                        theme_line_color: "Custom"
                        line_color: (0.1, 0.2, 0.4, 1)
                        line_width: 1.5
                        theme_bg_color: "Custom"
                        radius: [dp(10),]
                        padding: [dp(12),]
                        md_bg_color: (1, 1, 1, 1)
                        focus_color: (0, 0, 0, 0)
                        clip: True
                        MDIcon:
                            id: tgl_icon
                            icon: "calendar"
                            theme_icon_color: "Custom"
                            icon_color: (0.1, 0.2, 0.4, 1)
                            pos_hint: {"center_x": 0.2, "center_y": 0.5}
                            theme_font_size: "Custom"
                            font_size: "26sp"
                        MDTextField:
                            id: tanggal_lahir
                            mode: "outlined"
                            text: "Masukkan tanggal lahir"
                            size_hint_x: 1
                            radius: [dp(10),]
                            pos_hint: {"center_x": .5, "center_y": .465}
                            theme_text_color: "Custom"
                            text_color_normal: (0.1, 0.2, 0.4, 0.85)
                            text_color_focus: (0.1, 0.2, 0.4, 1)
                            theme_font_name: "Custom"
                            theme_font_size: "Custom"
                            font_size: "13.5sp"
                            font_name: "Poppins-Regular"
                            theme_line_color: "Custom"
                            line_color_normal: (0, 0, 0, 0)
                            line_color_focus: (0, 0, 0, 0)
                            theme_bg_color: "Custom"
                            fill_color_normal: (1, 1, 1, 0)
                            fill_color_focus: (1, 1, 1, 0)
                            on_focus:
                                root.update_text(self, self.focus, "Masukkan tanggal lahir")
                                root.show_date_picker(self, self.focus)
                            readonly: True
                            on_text: self.error = False
                
                MDBoxLayout:
                    orientation: "vertical"
                    spacing: dp(2)
                    size_hint_y: None
                    height: self.minimum_height
                    MDLabel:
                        text: "Daerah Asal - Kabupaten (Sesuai KTP)"
                        size_hint_y: None
                        height: self.texture_size[1]
                        theme_text_color: "Custom"
                        text_color: (0.1, 0.2, 0.4, 1)
                        theme_font_size: "Custom"
                        font_size: "11sp"
                        halign: "left"
                        theme_font_name: "Custom"
                        font_name: "Poppins-SemiBold"
                        size_hint_x: None
                        width: dp(300)
                        padding: [dp(24), 0]
                    MDCard:
                        id: asal_card
                        style: "outlined"
                        size_hint: .87, None
                        height: dp(54)
                        pos_hint: {"center_x": 0.5, "center_y": 0.7}
                        theme_line_color: "Custom"
                        line_color: (0.1, 0.2, 0.4, 1)
                        line_width: 1.5
                        theme_bg_color: "Custom"
                        radius: [dp(10),]
                        padding: [dp(12),]
                        md_bg_color: (1, 1, 1, 1)
                        focus_color: (0, 0, 0, 0)
                        clip: True
                        MDIcon:
                            id: asal_icon
                            icon: "home-map-marker"
                            theme_icon_color: "Custom"
                            icon_color: (0.1, 0.2, 0.4, 1)
                            theme_font_size: "Custom"
                            font_size: "28sp"
                            pos_hint: {"center_x": 0.2, "center_y": 0.5}
                        MDTextField:
                            id: asal
                            mode: "outlined"
                            text: "Masukkan daerah asal"
                            size_hint_x: 1
                            radius: [dp(10),]
                            pos_hint: {"center_x": .5, "center_y": .465}
                            theme_text_color: "Custom"
                            text_color_normal: (0.1, 0.2, 0.4, 0.85)
                            text_color_focus: (0.1, 0.2, 0.4, 1)
                            theme_font_name: "Custom"
                            theme_font_size: "Custom"
                            font_size: "13.5sp"
                            font_name: "Poppins-Regular"
                            theme_line_color: "Custom"
                            line_color_normal: (0, 0, 0, 0)
                            line_color_focus: (0, 0, 0, 0)
                            theme_bg_color: "Custom"
                            fill_color_normal: (1, 1, 1, 0)
                            fill_color_focus: (1, 1, 1, 0)
                            on_focus: root.update_text(self, self.focus, "Masukkan daerah asal")
                            on_text: self.error = False
                
                MDBoxLayout:
                    orientation: "vertical"
                    spacing: dp(2)
                    size_hint_y: None
                    height: self.minimum_height
                    MDLabel:
                        text: "Alamat Lengkap - Tempat Tinggal Saat Ini"
                        size_hint_y: None
                        height: self.texture_size[1]
                        theme_text_color: "Custom"
                        text_color: (0.1, 0.2, 0.4, 1)
                        theme_font_size: "Custom"
                        font_size: "11sp"
                        halign: "left"
                        theme_font_name: "Custom"
                        font_name: "Poppins-SemiBold"
                        size_hint_x: None
                        width: dp(300)
                        padding: [dp(24), 0]
                    MDCard:
                        id: alamat_card
                        style: "outlined"
                        size_hint: .87, None
                        height: dp(54)
                        pos_hint: {"center_x": 0.5, "center_y": 0.7}
                        theme_line_color: "Custom"
                        line_color: (0.1, 0.2, 0.4, 1)
                        line_width: 1.5
                        theme_bg_color: "Custom"
                        radius: [dp(10),]
                        padding: [dp(12),]
                        md_bg_color: (1, 1, 1, 1)
                        focus_color: (0, 0, 0, 0)
                        clip: True
                        MDIcon:
                            id: alamat_icon
                            icon: "home"
                            theme_icon_color: "Custom"
                            icon_color: (0.1, 0.2, 0.4, 1)
                            theme_font_size: "Custom"
                            font_size: "28sp"
                            pos_hint: {"center_x": 0.2, "center_y": 0.5}
                        MDTextField:
                            id: alamat
                            mode: "outlined"
                            text: "Masukkan alamat lengkap"
                            size_hint_x: 1
                            radius: [dp(10),]
                            pos_hint: {"center_x": .5, "center_y": .465}
                            theme_text_color: "Custom"
                            text_color_normal: (0.1, 0.2, 0.4, 0.85)
                            text_color_focus: (0.1, 0.2, 0.4, 1)
                            theme_font_name: "Custom"
                            theme_font_size: "Custom"
                            font_size: "13.5sp"
                            font_name: "Poppins-Regular"
                            theme_line_color: "Custom"
                            line_color_normal: (0, 0, 0, 0)
                            line_color_focus: (0, 0, 0, 0)
                            theme_bg_color: "Custom"
                            fill_color_normal: (1, 1, 1, 0)
                            fill_color_focus: (1, 1, 1, 0)
                            on_focus: root.update_text(self, self.focus, "Masukkan alamat lengkap")
                            on_text: self.error = False
                
        MDButton:
            style: "outlined"
            theme_bg_color: "Custom"
            md_bg_color: (1, 1, 1, 0)
            theme_line_color: "Custom"
            line_color: (0.1, 0.2, 0.4, 1)
            size: dp(135), dp(40)
            line_width: 2
            pos_hint: {"center_x": .5, "center_y": .125}
            theme_bg_color: "Custom"
            size_hint_y: None
            height: dp(47)
            radius: dp(20)
            on_release: root.next_signup()
            MDButtonText:
                text: "NEXT"
                theme_text_color: "Custom"
                text_color: (0.1, 0.2, 0.4, 1)
                theme_font_name: "Custom"
                theme_font_size: "Custom"
                font_size: "16sp"
                padding: [dp(35), 0]
                font_name: "Poppins-Bold"
                halign: "center"
                pos_hint: {"center_x": .5, "center_y": .5}       